
public class ChemicalElementApp {
	public static void main(String[] args) {
		ChemicalElement el1 = new ChemicalElement();
		el1.atomicNum = 37;
		ChemicalElement el2 = new ChemicalElement();
		el2.atomicNum = 25;
		ChemicalElement el3 = new ChemicalElement();
		el3.atomicNum = 81;
		System.out.println(el1.isAlkaliMetal());
		System.out.println(el2.isTransitionMetal());
		System.out.println(el3.isMetal());
	}
}
